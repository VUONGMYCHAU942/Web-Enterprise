					<%- include('../components/sidebar/sidebar-manager.ejs') %> 
				<% } %>
		
				<% if (typeof(role) !== 'undefined' && role === 'coordinator') { %>
					<%- include('../components/sidebar/sidebar-coordinator.ejs') %> 
				<% } %>

				<% if (typeof(role) !== 'undefined' && role === 'staff') { %>
					<%- include('../components/sidebar/sidebar-staff.ejs') %> 
				<% } %>
				<!--end::Aside-->

				<!--begin::Wrapper-->
				<div class="wrapper d-flex flex-column flex-row-fluid" id="kt_wrapper">

					<!--begin::Header-->
					<%- include('../components/header/index.ejs') %> 
					<!--end::Header-->
					
