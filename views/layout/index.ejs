					<div class="content d-flex flex-column flex-column-fluid fs-6" id="kt_content">
						<!--begin::Container-->
						<div class="container" id="kt_content_container">
							<div class="card">
								<div class="card-body">
									<%- body %> 
								</div>
							</div>
						</div>
						<!--end::Container-->
					</div> 
					<!--end::Content-->
					<!--begin::Footer-->
					<%- include('../components/footer.ejs') %> 
					<!--end::Footer-->
				</div>
